<h1 class="text-center mt-4">Edit One Category Name</h1>

<div class="container">
  <div class="row justify-content-center">
    <div class="col-8">
      <%= render 'form' %>
    </div>
  </div>
</div>

<div class="row justify-content-center">
  <div class="mb-3">
